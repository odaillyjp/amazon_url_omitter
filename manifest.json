{
  "manifest_version": 2,
  "name"            : "Amazon url omitter",
  "version"         : "1.0",
  "description"     : "Omit Amazon.com product page's url.",
  "permissions"     : [
    "tabs",
    "http://www.amazon.co.jp/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon"  : "icon.png",
    "default_title" : "Amazon url omitter"
  }
}
